  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "concurrently": "^2.0.0",
    "live-server": "^0.9.2",
    "node-sass": "^3.4.2",
    "watch": "^0.17.1"
  }
}
